{
  "build": {
    "env": {
      "VITE_API_KEY": "@dragonball_api_key",
      "VITE_API_BASE_URL": "https://dragonball-api.com/api"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://dragonball-api.com/api/$1",
      "headers": {
        "Authorization": "Bearer $VITE_API_KEY"
      }
    }
  ]
}
